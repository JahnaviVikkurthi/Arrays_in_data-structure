#include<stdio.h>
void main()
{
    int a[7]={1,2,3,4,5,8,9};
    int b[7];
    int n=7,i;
    for(i=0;i<n;i++)
    {
        b[i]=a[i];
    }
    printf("first array:\n");
    for(i=0;i<n;i++)
    {
        printf("%d\t",a[i]);
    }
    printf("second array:\n");
    for(i=0;i<n;i++)
    {
    printf("%d\t",b[i]);
    }
}
