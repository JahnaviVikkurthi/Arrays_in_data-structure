#include<stdio.h>
void main()
{
    int n,odd_count,even_count,i;
    int arr[]={1,7,6,9,20,33,56,118,6,44};
    n=sizeof(arr)/sizeof(arr[0]);
    odd_count=0;
    even_count=0;
    for(i=0;i<n;i++)
    {
    if(arr[i]%2==0)
    {
        even_count++;
    }
    else
    {
        odd_count++;
    }
    }
printf("odd count is %d\n",odd_count);
printf("even count is %d\n",even_count);
}
