#include <stdio.h>
int main()
{
int array[] = {11,21,1,43,98,61,77,99}; 
int max = array[0];
int min = array[0];
int i;
for(i = 1; i < sizeof(array)/sizeof(array[0]); i++)
{
if(array[i] > max)
{
max = array[i];
}
if(array[i] < min)
{
min = array[i];
}
}
printf("Maximum element = %d\n", max);
printf("Minimum element = %d\n", min);
return 0;
}
